{
  "duplex": {
    "tts_model": "edge-tts",
    "tts_voice": "ar-SA-ZariyahNeural",
    "tts_streaming_enabled": true,
    "tts_chunk_size_ms": 40,
    "tts_format": "opus",
    "echo_correlation_threshold": 0.15,
    "echo_spectral_threshold": 0.12,
    "echo_vad_threshold": 0.05,
    "echo_suppression_enabled": true,
    "echo_adaptive_threshold": true,
    "jitter_buffer_target_ms": 60,
    "jitter_buffer_max_ms": 200,
    "jitter_buffer_adaptive": true,
    "duplex_mode": "full",
    "barge_in_enabled": true,
    "barge_in_sensitivity": 0.3,
    "barge_in_delay_ms": 300,
    "audio_buffer_size_ms": 10000,
    "pcm_buffer_size_samples": 16000,
    "max_concurrent_sessions": 10,
    "session_timeout_ms": 300000
  },
  "system": {
    "gpu_enabled": true,
    "gpu_memory_fraction": 0.8,
    "gpu_memory_growth": true,
    "log_level": "INFO",
    "log_format": "json",
    "log_file": null,
    "cache_dir": "./cache",
    "model_cache_size_gb": 10,
    "api_timeout_s": 30,
    "max_request_size_mb": 100,
    "metrics_enabled": true,
    "tracing_enabled": false,
    "health_check_interval_s": 30
  },
  "connection_pool": {
    "max_pool_size": 100,
    "min_pool_size": 5,
    "max_idle_time_seconds": 600,
    "health_check_interval_seconds": 30,
    "acquisition_timeout_seconds": 10,
    "enable_metrics": true,
    "enable_circuit_breaker": true,
    "max_message_queue_size": 100
  },
  "circuit_breaker": {
    "enabled": true,
    "failure_threshold": 5,
    "recovery_timeout_seconds": 60.0,
    "success_threshold": 3,
    "failure_rate_threshold": 0.5,
    "minimum_requests": 10,
    "timeout_seconds": 30.0,
    "max_retry_attempts": 3,
    "backoff_multiplier": 2.0,
    "max_backoff_seconds": 300.0,
    "health_check_enabled": true,
    "health_check_interval_seconds": 30.0
  },
  "performance_monitoring": {
    "enabled": true,
    "collection_interval_seconds": 10.0,
    "metrics_retention_seconds": 3600,
    "anomaly_detection_enabled": true,
    "alerting_enabled": true,
    "system_metrics_enabled": true,
    "circuit_breaker_monitoring": true,
    "connection_pool_monitoring": true,
    "custom_metrics_enabled": true,
    "thresholds": {
      "cpu_usage_warning_threshold": 80.0,
      "cpu_usage_critical_threshold": 95.0,
      "memory_usage_warning_threshold": 85.0,
      "memory_usage_critical_threshold": 95.0,
      "response_time_warning_threshold_ms": 1000.0,
      "response_time_critical_threshold_ms": 5000.0,
      "error_rate_warning_threshold": 0.05,
      "error_rate_critical_threshold": 0.10
    },
    "historical_data": {
      "enable_historical_data": true,
      "historical_data_points": 1000,
      "data_aggregation_interval_seconds": 60.0
    }
  },
  "models": {
    "default_asr_model": "openai/whisper-large-v3",
    "default_llm_model": "microsoft/DialoGPT-large",
    "default_tts_model": "edge-tts",
    "model_load_timeout_s": 120,
    "model_unload_delay_s": 300,
    "preload_models": [],
    "quantization_enabled": true,
    "quantization_bits": 8
  }
}